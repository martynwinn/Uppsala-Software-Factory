#
OBJ=	dataman.o ../gklib/osx_kleylib  ../ccp4libs_latest_m32_osx/libccp4f.a  ../ccp4libs_latest_m32_osx/libccp4c.a
EXEC =	dataman
INCL=	Makefile_osx
SUBS = 
SUBS = dataman_subs.o
INCL = Makefile_osx dataman.incl
##STATIC## STATIC = -static

fc =	 gfortran -DOSX

opt= -O -u -check_bounds -ffixed-line-length-132 -m32 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) -m32 ; strip $(EXEC)

dataman.o:	dataman.fpp $(INCL)
	$(fc) $(opt) -c dataman.fpp

dataman_subs.o:	dataman_subs.fpp $(INCL)
			$(fc) $(opt) -c dataman_subs.fpp
